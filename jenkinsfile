node {
    stage('SCM') {
	echo 'Gathering code from version control'
	git branch: 'main', url: 'https://github.com/defernandezo2/homework'
        sh 'pwd'
    }
    stage('Build') {
    echo 'Building....'
    }
    stage('Test') {
       try{
               echo 'Building....'
               sh './gradlew cucumber'
               }
               catch( exception){
               throw exception
               }
    }
    stage('Deploy') {
        echo 'Deploying....'
    }
}